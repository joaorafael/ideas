@using WebIdeas.ViewModels
@{
    var contributers = ((IContributers) @Model).Contributers;
    var ideas = ((IIdeas)@Model).Ideas;
}
<div id="tops">
    <div class="centered">
        <div id="contribuidores">
            <h1>Top 5 Contribuidores</h1>
            <ol>
                @foreach (var contributer in contributers)
                {
                    <li>
                        @Html.ActionLink(@contributer.Name, "Details", "Contributer", new { id = @contributer.Id }, null)
                    </li>
                }
            </ol>
        </div>
        <div id="ideias">
            <h1>Top 5 Ideias</h1>
            <ol>
                @foreach (var idea in ideas)
                {
                    <li>
                        @Html.ActionLink(@idea.Text, "Details", "Idea", new { id = @idea.Id }, null)
                    </li>
                }
            </ol>
        </div>
    </div>
</div>